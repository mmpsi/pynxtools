{
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusXRot]/name": "FocusXRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusXRot]/description": "Focus mirror, pitch angle (surface normal axis)",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusXRot]/value": "/scan1/attrs/FocusXRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusXRot]/value/@units": "mrad",

    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusYRot]/name": "FocusYRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusYRot]/description": "Focus mirror, yaw angle (vertical axis)",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusYRot]/value": "/scan1/attrs/FocusYRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusYRot]/value/@units": "mrad",

    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusZRot]/name": "FocusZRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusZRot]/description": "Focus mirror, roll angle (beam axis)",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusZRot]/value": "/scan1/attrs/FocusZRot",
    "/ENTRY[entry]/COLLECTION[positioners]/POSITIONER[FocusZRot]/value/@units": "mrad",

    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/COLLECTIONCOLUMN[collectioncolumn]/scheme": "angular dispersive",

    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/DETECTOR[detector]/amplifier_type": "MCP",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/DETECTOR[detector]/detector_type": "Phosphor+CMOS",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/DETECTOR[detector]/raw_data/@signal": "arb. units",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/DETECTOR[detector]/raw_data/raw": {
        "link": "/entry/image/image"
    },

    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/ENERGYDISPERSION[energydispersion]/pass_energy": "/scan1/attrs/PassEnergy",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/ENERGYDISPERSION[energydispersion]/pass_energy/@units": "eV",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/ENERGYDISPERSION[energydispersion]/scheme": "hemispherical",

    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/description": "EW4000",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/device_information/identifier": "ESxyz",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/device_information/model": "EW4000",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/device_information/vendor": "ScientaOmicron",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/work_function": "4.6",
    "/ENTRY[entry]/INSTRUMENT[instrument]/ELECTRONANALYSER[electronanalyser]/work_function/@units": "eV",

    "/ENTRY[entry]/INSTRUMENT[instrument]/beam_synchrotron/associated_source": {
        "link": "/entry/instrument/source_synchrotron"
    },
    "/ENTRY[entry]/INSTRUMENT[instrument]/beam_synchrotron/distance": 0.0,
    "/ENTRY[entry]/INSTRUMENT[instrument]/beam_synchrotron/distance/@units": "mm",
    "/ENTRY[entry]/INSTRUMENT[instrument]/beam_synchrotron/incident_energy": "/scan1/attrs/MonoEnergy",
    "/ENTRY[entry]/INSTRUMENT[instrument]/beam_synchrotron/incident_energy/@units": "eV",

    "/ENTRY[entry]/INSTRUMENT[instrument]/source_synchrotron/associated_beam": {
        "link": "/entry/instrument/beam_synchrotron"
    },
    "/ENTRY[entry]/INSTRUMENT[instrument]/source_synchrotron/name": "SLS/X03DA",
    "/ENTRY[entry]/INSTRUMENT[instrument]/source_synchrotron/type": "Synchrotron X-ray Source",

    "/ENTRY[entry]/SAMPLE[sample]/name": "general/sample",

    "/ENTRY[entry]/DATA[data]/@axes": ["energy"],
    "/ENTRY[entry]/DATA[data]/@energy_indices": 0,
    "/ENTRY[entry]/DATA[data]/@signal": "data",
    "/ENTRY[entry]/DATA[data]/data": "/scan1/ScientaSpectrum",
    "/ENTRY[entry]/DATA[data]/data/@units": "arb. units",
    "/ENTRY[entry]/DATA[data]/data/@long_name": "counts",
    "/ENTRY[entry]/DATA[data]/energy": {
        "link": "/entry/image/energy"
    },
    "/ENTRY[entry]/DATA[data]/energy/@type": "kinetic",
    "/ENTRY[entry]/DATA[data]/energy/@units": "eV",
    "/ENTRY[entry]/DATA[data]/energy/@long_name": "kinetic energy (eV)",

    "/ENTRY[entry]/DATA[image]/@axes": ["angular0", "energy"],
    "/ENTRY[entry]/DATA[image]/@angular0_indices": 0,
    "/ENTRY[entry]/DATA[image]/@energy_indices": 1,
    "/ENTRY[entry]/DATA[image]/@signal": "image",
    "/ENTRY[entry]/DATA[image]/image": "/scan1/ScientaImage",
    "/ENTRY[entry]/DATA[image]/image/@units": "arb. units",
    "/ENTRY[entry]/DATA[image]/energy": "/scan1/ScientaChannels",
    "/ENTRY[entry]/DATA[image]/energy/@type": "kinetic",
    "/ENTRY[entry]/DATA[image]/energy/@units": "eV",
    "/ENTRY[entry]/DATA[image]/angular0": {
        "eval": "np.linspace(arg0[0], arg1[0], int(arg2[0]))",
        "arg0": "/scan1/attrs/ScientaSliceBegin",
        "arg1": "/scan1/attrs/ScientaSliceEnd",
        "arg2": "/scan1/attrs/ScientaNumSlices"
    },
    "/ENTRY[entry]/DATA[image]/angular0/@units": "°",
    "/ENTRY[entry]/DATA[image]/angular0/@long_name": "emission angle (°)",

    "/ENTRY[entry]/USER[user]/affiliation": "general/pgroup",
    "/ENTRY[entry]/USER[user]/name": "general/authors",

    "/ENTRY[entry]/definition": "NXmpes",
    "/ENTRY[entry]/definition/@version": "None",
    "/ENTRY[entry]/start_time": {
        "parse_string":  "/logs/logs",
        "index": "0",
        "regexp": "^[0-9.:/]+ [0-9.:/]+",
        "datetime": "%d/%m/%y %H:%M:%S.%f",
        "timezone": "Europe/Zurich"
    },
    "/ENTRY[entry]/end_time": {
        "parse_string":  "/logs/logs",
        "index": "-1",
        "regexp": "^[0-9.:/]+ [0-9.:/]+",
        "dateutil": "dmy",
        "timezone": "Europe/Zurich"
    },
    "/ENTRY[entry]/title": "None",
    "/ENTRY[entry]/@default": "data",
    "/@default": "entry"
}
